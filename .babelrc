{
	"presets": [
		"@babel/typescript",
		["@babel/preset-env", { "modules": false }],
		"@babel/preset-react"
	],
	"plugins": [
		"react-hot-loader/babel",
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	],
	"env": {
		"production": {
			"presets": ["minify"]
		},
		"test": {
			"presets": ["@babel/preset-env", "@babel/preset-react"]
		}
	}
}
